<nav aria-label="{{ meta.navigation.ariaTop }}">
  <ul class="mainnav" role="list">
    {% for item in navigation.top %}
      <li>
        <a
          href="{{ item.url }}"
          {{
            helpers.getLinkActiveState(item.url, page.url)
            | safe
          }}
          >{{ item.text }}</a
        >
      </li>
    {% endfor %}
  </ul>
</nav>
